{
    "name": "magento/magento-semver",
    "description": "Magento Semantic Version Checker",
    "version": "13.0.0-beta7",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "bin": ["bin/svc"],
    "require": {
        "php": "~8.1.0||~8.2.0||~8.3.0||~8.4.0",
        "ext-json": "*",
        "laminas/laminas-stdlib": "^3.18",
        "nikic/php-parser": "^4.19",
        "phpstan/phpdoc-parser": "^0.5.5",
        "sabre/xml": "~4.0.5",
        "symfony/console": "~6.4.17",
        "symfony/string": "~6.4.15",
        "tomzx/php-semver-checker": "^0.16.0",
        "wikimedia/less.php": "^5.1"
    },
    "require-dev": {
        "ext-dom": "*",
        "phpunit/phpunit": "~10.5.28",
        "sebastian/cli-parser": "^2.0.1",
        "squizlabs/php_codesniffer": "~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Magento\\SemanticVersionChecker\\": "src/",
            "Magento\\SemanticVersionChecker\\Test\\": "dev/tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }
    },
    "archive": {
        "exclude": [
            ".github",
            ".gitignore",
            "dev/tests",
            "composer.lock"
        ]
    }
}
